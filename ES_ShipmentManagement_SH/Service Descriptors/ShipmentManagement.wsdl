  <wsdl:message name="UpdateShipmentRequest">
    <wsdl:part element="xsd1:updateShipmentReq" name="parameters"/>
  </wsdl:message>
  <wsdl:message name="UpdateShipmentResponse">
    <wsdl:part element="xsd1:updateShipmentRes" name="parameters"/>
  </wsdl:message>
  <wsdl:message name="UpdateShipmentFault">
  	<wsdl:part name="parameters" element="xsd2:Fault"></wsdl:part>
  </wsdl:message>
  <wsdl:message name="TrackShipmentRequest">
  	<wsdl:part name="parameters" element="xsd1:TrackShipReq"></wsdl:part>
  </wsdl:message>
  <wsdl:message name="TrackShipmentResponse">
  	<wsdl:part name="parameters" element="xsd1:TrackShipRes"></wsdl:part>
  </wsdl:message>
  <wsdl:message name="TrackShipmentFault">
  	<wsdl:part name="fault" element="xsd2:Fault"></wsdl:part>
