<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.EniqueSolution.com/abstarct/V1/payments/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="payments" targetNamespace="http://www.EniqueSolution.com/abstarct/V1/payments/" xmlns:xsd1="http://www.example.org/Payments" xmlns:xsd2="http://www.eniquesolution.com/E-Commerce/Common">
  <wsdl:types>
    
    <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    	<xsd:import namespace="http://www.example.org/Payments"
    		schemaLocation="../Schemas/Payments.xsd">
    	</xsd:import>
    	<xsd:import
    		namespace="http://www.eniquesolution.com/E-Commerce/Common"
    		schemaLocation="../../ES_Common_Model/Schemas/Common.xsd">
    	</xsd:import></xsd:schema><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.EniqueSolution.com/abstarct/V1/payments/">
	</xsd:schema></wsdl:types>
  <wsdl:message name="MakePaymentsRequest">
    <wsdl:part element="xsd1:MakePaymentsReq" name="parameters"/>
  </wsdl:message>
  <wsdl:message name="MakePaymentsResponse">
    <wsdl:part element="xsd1:MakePaymentRes" name="parameters"/>
  </wsdl:message>
  <wsdl:message name="MakePaymentsFault">
  	<wsdl:part name="parameters" element="xsd2:Fault"></wsdl:part>
  </wsdl:message>
  <wsdl:portType name="payments">
    <wsdl:operation name="MakePayments">
      <wsdl:input message="tns:MakePaymentsRequest"/>
      <wsdl:output message="tns:MakePaymentsResponse"/>
            <wsdl:fault name="fault" message="tns:MakePaymentsFault"></wsdl:fault>
        </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="paymentsSOAP" type="tns:payments">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="MakePayments">
      <soap:operation soapAction="http://www.EniqueSolution.com/abstarct/V1/payments/MakePayments"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="payments">
    <wsdl:port binding="tns:paymentsSOAP" name="paymentsSOAP">
      <soap:address location="http://www.example.org/"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
